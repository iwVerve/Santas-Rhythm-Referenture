        <arguments>
          <argument>
            <kind>1</kind>
            <string>hold_dir = scrButtonCheck(KEY.RIGHT);
if !hold_dir {
    hold_dir = -scrButtonCheck(KEY.LEFT);
}

var lens_target = 400 + global.lens_step * hold_dir;
var diff = lens_target - lens_x;
lens_x += sign(diff) * min(abs(diff), 28);

if scrButtonCheckPressed(KEY.SHOOT) {
    var best_note = noone;
    with(objNoteParent) {
        if best_note == noone || progress &gt; best_note.progress {
            best_note = id;
        }
    }
    if best_note != noone &amp;&amp; best_note.grade &gt; 0 {
        if best_note.dir == hold_dir || best_note.dir == -2 {
            if best_note.dir == -2 {
                award_grade(best_note.grade, 40);
            }
            else {
                award_grade(best_note.grade);
            }
            lens_flash = lens_flash_duration;
        }
        else {
            award_grade(0);
        }
        instance_destroy(best_note);
    }
    else {
        award_grade(0);
    }
}

time += 1;
if time == 0 {
    scrPlayMusic(global.song, false);
}

//if time &gt; (beat + bit/bits) * 3000 / bpm {
if true {
    /*bit += 1;
    if bit &gt;= bits {
        bit -= bits;
        beat += 1;
    }*/
    
    while(data[# next_attack, 0] != -1 &amp;&amp; time &gt;= data[# next_attack, 0]) {
        switch(data[# next_attack, 1]) {
            case 0:
                with(instance_create(0, 0, objNoteLine)) {
                    dir = -1;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 1:
                with(instance_create(0, 0, objNoteLine)) {
                    dir = 1;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 2:
                with(instance_create(0, 0, objNoteAimed)) {
                    dir = -1;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 3:
                with(instance_create(0, 0, objNoteAimed)) {
                    dir = 1;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 4:
                with(instance_create(0, 0, objNoteHorizontal)) {
                    dir = -1;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 5:
                with(instance_create(0, 0, objNoteHorizontal)) {
                    dir = 0;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 6:
                with(instance_create(0, 0, objNoteHorizontal)) {
                    dir = 1;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 7:
                with(instance_create(0, 0, objNoteBonus)) {
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 8:
                with(instance_create(0, 0, objNoteCircle)) {
                    dir = -1;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 9:
                with(instance_create(0, 0, objNoteCircle)) {
                    dir = 1;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 10:
                with(instance_create(0, 0, objNoteAimed)) {
                    dir = 0;
                    spawn = other.data[# other.next_attack, 2];
                }
                break;
            case 11:
                if global.song == "Manic" {
                    global.beatManic = true;
                }
                with(instance_create(384, 384, objWarp)) {
                    roomTo = rHub;
                }
                ended = true;
                break;
            case 12:
                instance_create(384, 256, objTutorial);
                break;
        }
        next_attack++;
    }
}

if display_time &gt; 0 {
    display_scale = 1 + 2 * power(max(display_time / display_duration - 0.5, 0), 4);
    display_time--;
}

if lens_flash &gt; 0 {
    lens_flash -= 1;
}

if points - display_points &gt; 10 {
    display_points = floor(lerp(display_points, points, 0.2) + 0.5);
}
else if display_points &lt; points {
    display_points += 1;
}

